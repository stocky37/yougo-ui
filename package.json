{
	"private": true,
	"name": "yougo-ui",
	"version": "0.1.0",
	"scripts": {
		"preapi": "cp api/db.json api/live.json",
		"api": "json-server -r api/routes.json -p 8080 api/live.json",
		"api:save": "cp api/live.json api/db.json",
		"api:watch": "npm run api -- -w",
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"preformat": "prettier-package-json --write",
		"format": "prettier --write .",
		"preformat:check": "prettier-package-json -l",
		"format:check": "prettier --check .",
		"lint": "eslint --ext js,jsx src",
		"lint:clear": "rm -rf node_modules/.cache/eslint-loader",
		"lint:fix": "npm run lint -- --fix",
		"prestart": "npm run lint:clear",
		"start": "react-scripts start",
		"test": "react-scripts test"
	},
	"dependencies": {
		"@material-ui/core": "^4.0.0",
		"@material-ui/icons": "^4.0.0",
		"axios": "^0.19.2",
		"classnames": "^2.2.5",
		"copy-to-clipboard": "^3.0.8",
		"lodash": "^4.17.20",
		"material-ui-popup-state": "^1.5.1",
		"material-ui-search-bar": "^1.0.0",
		"material-ui-snackbar-provider": "^1.4.0",
		"normalizr": "^3.2.4",
		"object-diff": "^0.0.4",
		"prop-types": "^15.6.0",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-hook-form": "^4.0.1",
		"react-modal-hook": "^2.0.0",
		"react-query": "^2.5.14",
		"react-redux": "^7.0.0",
		"redux": "^4.0.0",
		"redux-actions": "^2.3.0",
		"redux-api-middleware": "^3.0.1",
		"redux-batched-actions": "^0.3.0",
		"redux-devtools-extension": "^2.13.2",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.2.0",
		"reselect": "^4.0.0",
		"typeface-roboto": "^0.0.75",
		"validator": "^12.0.0"
	},
	"devDependencies": {
		"@material-ui/codemod": "^4.5.0",
		"@testing-library/jest-dom": "^5.11.3",
		"@testing-library/react": "^10.4.8",
		"@testing-library/user-event": "^12.1.1",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.7.0",
		"eslint-plugin-simple-import-sort": "^5.0.3",
		"husky": "^4.2.5",
		"json-server": "^0.16.1",
		"lint-staged": "^10.2.11",
		"prettier": "^2.0.5",
		"prettier-package-json": "^2.1.3",
		"react-scripts": "3.4.3"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx}": "eslint --fix",
		"*.{js,jsx,json,yaml,yml,md,css}": "prettier --write"
	}
}
